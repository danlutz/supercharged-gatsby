{
  "name": "supercharged-gatsby",
  "version": "1.0.0",
  "description": "Makes your developer experience great again",
  "author": "Daniel Lutz <daniel-lutz@live.at>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danlutz/supercharged-gatsby.git"
  },
  "scripts": {
    "clean": "rimraf .cache public",
    "build": "npm run clean && npm run lambda-build && gatsby build",
    "lambda-serve": "netlify-lambda serve lambda/src -c lambda/webpack.lambda.js",
    "lambda-build": "netlify-lambda build lambda/src",
    "develop": "npm run clean && concurrently --kill-others \"npm run lambda-serve\" \"gatsby develop\"",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.ts,.tsx src lambda/src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|html|css|less|scss|ts|tsx|md|mdx|graphql|vue|py|java|php)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "jest": "jest",
    "test": "jest --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cloudinary-core": "^2.7.4",
    "cloudinary-react": "^1.1.4",
    "gatsby": "^2.15.6",
    "gatsby-plugin-google-analytics": "^2.1.14",
    "gatsby-plugin-layout": "^1.1.6",
    "gatsby-plugin-manifest": "^2.2.13",
    "gatsby-plugin-netlify-cms": "^4.1.13",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^2.1.13",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-source-filesystem": "^2.1.20",
    "gatsby-transformer-remark": "^2.6.21",
    "intl": "^1.2.5",
    "netlify-cms-app": "^2.9.7",
    "netlify-cms-media-library-cloudinary": "^1.3.3",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-lambda": "^8.10.31",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@types/reactstrap": "^8.0.4",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-jest": "^24.9.0",
    "concurrently": "^4.1.2",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-config": "^24.9.0",
    "lint-staged": "^9.2.5",
    "netlify-lambda": "^1.6.3",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  }
}
