{
  "name": "supercharged-gatsby",
  "version": "1.0.0",
  "description": "",
  "author": "Daniel Lutz <daniel-lutz@live.at>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .cache public",
    "build": "npm run clean && npm run lambda-build && gatsby build",
    "lambda-serve": "netlify-lambda serve lambda/src -c lambda/webpack.lambda.js",
    "lambda-build": "netlify-lambda build lambda/src",
    "develop": "npm run clean && concurrently --kill-others \"npm run lambda-serve\" \"gatsby develop\"",
    "start": "npm run develop",
    "lint": "eslint --ext .js,.ts,.tsx src lambda/src",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|html|css|less|scss|ts|tsx|md|mdx|graphql|vue|py|java|php)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier -- --list-different",
    "jest": "jest",
    "test": "jest --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cloudinary-core": "^2.6.3",
    "cloudinary-react": "^1.1.1",
    "gatsby": "^2.11.6",
    "gatsby-plugin-google-analytics": "^2.1.1",
    "gatsby-plugin-layout": "^1.1.0",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.1",
    "gatsby-transformer-remark": "^2.5.0",
    "netlify-cms-app": "^2.9.5",
    "netlify-cms-media-library-cloudinary": "^1.3.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "reactstrap": "^8.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-lambda": "^8.10.27",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/reactstrap": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-jest": "^24.8.0",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.2",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^2.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^8.2.1",
    "netlify-lambda": "^1.4.13",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
